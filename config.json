{
  "name": "Zigbee2mqtt",
  "version": "1.22.1-1",
  "slug": "zigbee2mqtt",
  "description": "Use your ZigBee devices without the vendor's bridge or gateway",
  "uart": true,
  "url": "https://github.com/zigbee2mqtt/hassio-zigbee2mqtt/tree/master/zigbee2mqtt",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "ingress": true,
  "panel_icon": "mdi:zigbee",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "8485/tcp": 8485
  },
  "ports_description": {
    "8485/tcp": "Socat tcp-listen port"
  },
  "options": {
  },
  "schema": {
  },
  "image": "zigbee2mqtt/zigbee2mqtt-{arch}"
}

