{
  "name": "LarTec",
  "version": "1.0.7",
  "url": "https://github.com/lartec/hub",
  "slug": "lartec-hub",
  "description": "LarTec",
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "manager",
  "uart": true,
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "ingress": true,
  "map": [
    "config:rw",
    "share:rw"
  ],
  "ports": {
    "4000/tcp": 4000
  },
  "ports_description": {
    "4000/tcp": "LarTec Hub tcp-listen port"
  },
  "options": {
  },
  "schema": {
  },
  "image": "lartec/hub-{arch}"
}
